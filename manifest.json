{
  "manifest_version": 3,
  "name": "Mizito Task Filter",
  "version": "1.5",
  "description": "فیلتر تسک‌های چند نفر با حذف نام‌های تکراری یا ترکیب‌شده",
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon_16.png",
      "32": "icon_32.png",
      "48": "icon_48.png",
      "128": "icon_128.png"
    }
  },
  "host_permissions": [
    "https://office.mizito.ir/*"
  ],
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  }
}